	"stepCommands": {
		"StartProcess": {
			"type": "User",
			"permission": "StartProcessPermission",
			"eventHandlers": [{
					"handler": "UpdateMilestone",
					"milestoneCode": "PPPP",
					"milestoneStatus": "Completed"
				},
				{
					"handler": "AddPhaseHistory",
					"descriptionTemplate": "op.phasehistory.StartProcess"
				}
			]
		},
		"SubmitToDivisionChief": {
			"type": "User",
			"permission": "SubmitToDivisionChiefPermission",
			"eventHandlers": [{
					"handler": "MoveToNextStep"
				},
				{
					"handler": "SendNotification",
					"notificationCode": "op.pp.001",
					"destinationRoles": [
						"Division Chief"
					]
				},
				{
					"handler": "UpdateMilestone",
					"milestoneCode": "PPDC",
					"milestoneStatus": "Completed"
				},
